:root {
  /* VoogStyle
    "pathI18n": ["content"],
    "titleI18n": "background_color",
    "editor": "colorPicker",
    "scope": "global"
  */
  --content-background-color: #eee;
  /* VoogStyle
  "pathI18n": ["content", "sub_menu", "normal"],
  "titleI18n": "size",
  "editor": "rangePicker",
  "min": 12,
  "max": 25,
  "unit": "px",
  "featured": true,
  "scope": "global"
*/
  --menu-sub-font-size: 20px;
  /* VoogStyle
    "pathI18n": ["content", "sub_menu", "normal"],
    "titleI18n": "color",
    "type": "text",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global",
    "boundVariables": [
      "--menu-sub-hover-color",
      "--menu-sub-active-color"
    ]
  */
  --menu-sub-color: #000;
  /* VoogStyle
    "pathI18n": ["content", "sub_menu", "hover"],
    "titleI18n": "color",
    "type": "text",
    "editor": "colorPicker",
    "scope": "global",
    "boundVariables": [
      "--menu-sub-active-color"
    ]
  */
  --menu-sub-hover-color: #000;
  /* VoogStyle
    "pathI18n": ["content", "sub_menu", "hover"],
    "titleI18n": "background_color",
    "editor": "colorPicker",
    "scope": "global"
  */
  --submenu-link-background-color: #eee077;
  /* VoogStyle
    "pathI18n": ["content", "sub_menu", "active"],
    "titleI18n": "color",
    "type": "text",
    "editor": "colorPicker",
    "scope": "global"
  */
  --menu-sub-active-color: #000;
  /* VoogStyle
    "pathI18n": ["content", "link", "normal"],
    "titleI18n": "color",
    "type": "text",
    "editor": "colorPicker",
    "featured": true,
    "scope": "global",
    "boundVariables": [
      "--content-link-hover-color"
    ]
  */
  --content-link-color: #eee077;
}
